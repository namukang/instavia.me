{
  "name": "via.me Instant",
  "version": "0.0.1",
  "description": "Chrome extension for via.me",
  "permissions": [
    "https://api.via.me/*"
  ],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["contentscript.js"]
  },
  {
    "matches": [""],
    "js": ["oauth2/oauth2_inject.js"],
    "run_at": "document_start"
  }
  ],
  "options_page": "options.html"
}
